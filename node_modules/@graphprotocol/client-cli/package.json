{
  "name": "@graphprotocol/client-cli",
  "version": "0.0.3",
  "description": "",
  "sideEffects": false,
  "peerDependencies": {
    "graphql": "^15.2.0 || ^16.0.0"
  },
  "dependencies": {
    "@graphql-mesh/cli": "0.66.0",
    "@graphql-mesh/graphql": "0.23.0",
    "tslib": "2.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/graphprotocol/graph-client.git",
    "directory": "packages/cli"
  },
  "keywords": [
    "thegraph",
    "graphql",
    "client"
  ],
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "typings": "index.d.ts",
  "typescript": {
    "definition": "index.d.ts"
  },
  "exports": {
    ".": {
